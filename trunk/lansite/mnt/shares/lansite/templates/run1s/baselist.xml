<?xml version="1.0" encoding="UTF-8"?>
<run1s ver="0.1" sn="{{ sn }}">
{% if err %}
	<error text="{{ err }}"/>
{% else %}
	{% for db in dbs %}
		<base host="{{ db.share.host.name }}" share="{{ db.share.name }}" path="{{ db.path }}" org="{{ db.org.name }}" type="{{ db.type.name }}" comments="{{ db.comments }}"/>
	{% endfor %}
{% endif %}
</run1s>
