	c0	NMTOKEN #FIXED "SubUsersInterfaceType"	// D CC0
	c2	CDATA #REQUIRED				// R CC2 - interface name (not NMTOKENS Bcouse of rus name)
	c3	CDATA #FIXED ""				// D CC3
>
<!ATTLIST scmds
	nm	NMTOKEN #FIXED "Commands"		// D name
	sz	NMTOKEN #REQUIRED			// D size - #
	pnm	NMTOKENS #IMPLIED			// ? parentname - for SUI only
	cnm	CDATA #REQUIRED				// R CommandName (can B "")
	nop	(0|1) #REQUIRED				// R NonOperations - Отключить меню "Операции" (0=no)
	nau	(0|1) #REQUIRED				// R NoAuth - Использовать при отключенной авторизации (0=no)
	cqty	NMTOKEN #REQUIRED			// D CommandsQty - кол-во команд
>
<!ATTLIST scmd
	cmd	NMTOKENS #REQUIRED			// R command
	ad	CDATA #REQUIRED				// R addon
>
<!ATTLIST	RigthType
	nm	NMTOKEN #FIXED "Page.2"			// D name
	c0	NMTOKEN #FIXED "RigthType"		// D CC0
	c2	CDATA #FIXED ""				// D CC2
	c3	CDATA #FIXED ""				// D CC3
>
<!ATTLIST rights
	nm	NMTOKEN #REQUIRED			// D name - Page.#
	c0	NMTOKEN #FIXED "RightsEditorType"	// D CC0
	c2	CDATA #REQUIRED				// R CC2 - role name
	c3	CDATA #FIXED ""				// D CC3
	sz	NMTOKEN #REQUIRED			// D # size - #
	sign	NMTOKEN #FIXED "6"			// ? # signature
	dummy2	CDATA #REQUIRED				// ? s some text
