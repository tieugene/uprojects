>
<!ATTLIST cmds
	nm	NMTOKEN #FIXED "Commands"		// D name
	sz	NMTOKEN #REQUIRED			// D size - #
	cnm	CDATA #FIXED ""				// R CommandName
	nop	(0|1) #REQUIRED				// R NonOperations - Отключить меню "Операции" (0=no)
	nau	(0|1) #REQUIRED				// R NoAuth - Использовать при отключенной авторизации (0=no)
	cqty	NMTOKEN #REQUIRED			// D CommandsQty - кол-во команд
>
<!ATTLIST cmd
	id	NMTOKEN #REQUIRED			//   ID (?) = 0x0000Cxxx
	stip	CDATA #REQUIRED				// R Short Tip (Короткая подсказка)
	tip	CDATA #REQUIRED				// R Tip (Подсказка)
	mk	NMTOKEN #REQUIRED			// R metakey = 0x11 (Alt) | 0x09 (Ctrl) | 0x05 (Shift)
	sc	NMTOKEN #REQUIRED			// R scancode
	cmd	NMTOKENS #REQUIRED			// R command
	ad	CDATA #REQUIRED				// R addon
>
<!ATTLIST	SubUsersInterfaceType
	no	NMTOKEN #REQUIRED			// ? number (digit) - uniq through *UserInterfaceTypes (?)
