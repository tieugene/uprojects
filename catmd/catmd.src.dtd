	c3	CDATA #FIXED ""				// D CC3
	sz	NMTOKEN #REQUIRED			// D size
>
<!ATTLIST pnl
	name	CDATA #REQUIRED				// R name
	qty	NMTOKEN #REQUIRED			// ? # - buttons qty
	pos	(59419|59420|59421|59422) #REQUIRED	// R # - position = 0xE81B + 0 (top) | 1 (left) | 2 (right) | 3 (button)
	show	(0|1) #REQUIRED				// R show (0=no)
	newline	(0|1) #REQUIRED				// R from new line (0=no)
>
<!ATTLIST btn						// ??? where button name ??? picture ???
	id	NMTOKEN #REQUIRED			// R IDREF? - command uid (?)
>
<!ATTLIST panelpic					//
